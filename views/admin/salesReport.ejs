    <form action="/admin/sales" method="get" onsubmit="return validateForm()">

  <!-- Input fields for start and end dates -->
  <div class="form-row mb-2">
    <div class="col-md-4">
      <label for="from">Start Date:</label>
      <input type="date" class="form-control" id="from" name="from" value="<%= from %>" required>
      <!-- Define the error message element -->
    </div>
    <div class="col-md-4">
      <label for="to">End Date:</label>
      <input type="date" class="form-control" id="to" name="to" value="<%= to %>" required min="<%= from %>">
      <div id="ending-error" class="error-message"></div> <!-- Define the error message element -->
    </div>
    
    <div class="col-md-4">
      <button type="submit" class="btn btn-primary">Apply Filter</button>
    </div>
  </div>
  <% if (salesReport.length) { %>
    <div class="text-center my-2 px-4">
      <button type="button" class="btn btn-primary btn-sm" id="sales-report-submit" onclick="pdf()">
        Download
      </button>
    </div>
  <% } %>

  <div class="bg-gradient-primary shadow-primary border-radius-lg pt-4 pb-3 text-center">
    <h6 class="text-white text-capitalize ps-3">Sales reports</h6>
  </div>

  <div id="tablePrint" class="table-responsive">
    <table class="table text-center table table-dark table-bordered table-striped">
      <thead>
        <tr class="border">
          <th scope="col" class="col-md-1" style="font-weight: 500;background-color: #000000;color: #E5E5E5;"><i>Date</i></th>
          <th scope="col" style="font-weight: 500;background-color: #000000;color: #E5E5E5;"><i> OrderId</i></th>
          <th scope="col" style="font-weight: 500;background-color: #000000;color: #E5E5E5;"><i>Product </i></th>
          <th scope="col" class="col-md-1" style="font-weight: 500;background-color: #000000;color: #E5E5E5;"><i> Quantity </i></th>
          <th scope="col" class="col-md-1" style="font-weight: 500;background-color: #000000;color: #E5E5E5;"><i> Payment </i></th>
          <th scope="col" style="font-weight: 500;background-color: #000000;color: #E5E5E5;"><i>Amount</i></th>
        </tr>
      </thead>

      <tbody>
        <%if(salesReport.length===0){%>
          <tr><h1> Not sales  </h1></tr>
          <%}%>
        <% salesReport.forEach((sale) => { %>
        <tr>
          <td><%= sale.order_date.toLocaleDateString() %> </td>
          <td><%= sale.order_id %></td>
          <td> <%= sale.products_details.title %></td>
          <td><%= sale.qty %></td>
          <td><%= sale.payment_method %></td>
          <td><%= sale.total_amount %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </div>
</form>

